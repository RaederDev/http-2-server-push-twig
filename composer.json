{
  "name": "raeder/craft-http2-server-push-twig",
  "description": "This plugin provides a convenient way to utilize Server Push via a twig filter",
  "type": "craft-plugin",
  "version": "2.0.0",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "craft-plugin",
    "http-2-server-push",
    "twig"
  ],
  "support": {
    "docs": "https://github.com/N0ps32/http-2-server-push-twig/blob/master/README.md",
    "issues": "https://github.com/N0ps32/http-2-server-push-twig/issues"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Benjamin RÃ¤der",
      "homepage": "https://www.raeder.technology/"
    }
  ],
  "require": {
    "craftcms/cms": "^4.0.0"
  },
  "autoload": {
    "psr-4": {
      "Raeder\\Technology\\ServerPush\\": "src/"
    }
  },
  "extra": {
    "name": "HTTP/2 Server Push Twig",
    "handle": "http-2-server-push-twig",
    "hasCpSettings": false,
    "hasCpSection": false,
    "changelogUrl": "https://raw.githubusercontent.com/N0ps32/http-2-server-push-twig/master/CHANGELOG.md",
    "class": "Raeder\\Technology\\ServerPush\\Http2ServerPushTwig"
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  }
}
